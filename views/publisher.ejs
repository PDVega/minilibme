<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Publisher</title>
  </head>
  <body>
    
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>Publisher</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <% data_publisher.forEach(publisher => { %>
        <tr>
          <td><%= publisher.id %></td>
          <td><%= publisher.name %></td>
          <td><a href="/publishers/edit/<%= publisher.id%>">Update</a> || <a href="/publishers/delete/<%= publisher.id%>" onclick="return confirm('Are you sure want to delete this Publisher?');">Delete</a></td>
        </tr>
        <% })%>
      </tbody>
    </table>
    
  </body>
</html>
